<h1>アップロードしたデータ</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>ID</th>
          <th>SKU</th>
          <th>受注数</th>
          <th>受注日</th>
        </tr>
      </thead>
      
      <tbody>
        <% @gods.each do |god| %>
          <tr>
            <td><%= god.id %></td>
            <td><%= god.sku %></td>
            <td><%= god.total %></td>
            <td><%= god.order_date %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>